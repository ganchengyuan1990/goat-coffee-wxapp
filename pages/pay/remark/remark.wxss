.flex {
    display: flex;
}

.title {
    margin-right: 30rpx;
    display: inline-block;
    min-width: 150rpx;
    text-align: right;
    font-size: 14px;
}

.section {
    margin: 30rpx;
    margin-bottom: 1.8rem;
    font-size: 14px;
    height: 1.8rem;
    line-height: 1.8rem;
    margin-left: 0rpx;
    /* margin-bottom: 0.9rem; */
