page {
  background-color: #fff;
}
.store-container {
  /* display: flex; *//* flex-direction: column; */
  height: 100vh;
  overflow: hidden;
}

.hd-wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 104rpx;
  padding-right: 30rpx;
}

.img-wrap {
  width: 100%;
}

.img-caption {
  display: block;
  width: 100%;
  height: auto;
  height: 260rpx !important;
}

.store-addr {
  padding-left: 58rpx;
  font-size: 24rpx;
  line-height: 1.4;
}

.store-addr .tt {
  position: relative;
}

.store-addr .tt::before {
  content: "";
  position: absolute;
  left: -12rpx;
  top: 50%;
  width: 16rpx;
  height: 24rpx;
  background: url('data:image/png;base64,') no-repeat 0 0/contain;
  transform: translate(-100%, -50%);
}

.store-addr .distance {
  color: #999;
  font-size: 20rpx;
}

.btn-delivery {
  flex: none;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 4rpx;
  border: 2rpx solid #7d7d7d;
  border-radius: 68rpx;
  color: #7d7d7d;
  font-size: 28rpx;
}

.btn-delivery .item {
  padding: 18rpx 24rpx;
  border-radius: 70rpx;
  line-height: 1;
}

.btn-delivery .item.active {
  color: #fff;
  background: #7d7d7d;
}

.list-wrap {
  display: flex;
  height: 100%;
}

.list-nav {
  width: 200rpx;
  background-color: #f5f5f5;
}

.list-nav .active {
  background-color: #fff;
}

.list-nav .list-nav-item {
  position: relative;
  height: 100rpx;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.list-nav .list-nav-item:last-child {
  margin-bottom: 120rpx;
}
.list-nav .list-nav-item .icon-crown {
  display: none;
}
.list-nav .list-nav-item.active .icon-crown {
  display: inline-block;
}
.list-nav .list-nav-item .tt {
  text-align: center;
  max-height: 80rpx;
  overflow: hidden;
  line-height: 1;
}

.list-cnt {
  flex-grow: 1;
  padding-left: 20rpx;
  font-size: 26rpx;
}
.list-cnt .title {
  font-weight: 700;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #eee;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.list-cnt .title-group {
  padding-bottom: 30rpx;
}

.list-cnt .title-group:last-child {
  margin-bottom: 120rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.list-detail {
  margin-bottom: 68rpx;
  display: flex;
}

.list-detail:last-child {
  margin-bottom: 0;
}

.list-detail .img {
  margin-right: 24rpx;
}

.list-detail .img image {
  display: block;
  width: 160rpx;
  height: 160rpx;
  border-radius: 8rpx;
}

.list-detail .info {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.info .name {
  /* line-height: 2; */
  font-size: 32rpx;
  font-weight: 400;
}

.info .note {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 20rpx;
  line-height: 1.4;
  color: #c6c6c6;
}

.price-count {
  display: flex;
  align-items: center;
  /* height: 50rpx;
  line-height: 50rpx; */
}

.price-count .price {
  font-size: 30rpx;
  line-height: 1;
  flex-grow: 1;
  color: #000;
}

.price-count .count {
  display: flex;
  line-height: 60rpx;
  flex-grow: 1;
  flex-direction: row-reverse;
}

.price-count .count .num {
  line-height: 50rpx;
  font-size: 30rpx;
}

.btn {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  font-size: 36rpx;
  font-weight: 400;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
  background-color: #d8d8d8;
  margin: 0 40rpx 0 16rpx;
}

.price-wrap .price {
  font-size: 36rpx;
}

.price-wrap .desc {
  font-size: 20rpx;
  color: #c6c6c6;
}

.counter-wrap {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-top: 38rpx;
}

.counter-wrap .counter {
  display: flex;
  align-items: center;
}

.counter-wrap .counter .num {
  padding: 0 20rpx;
  font-size: 22rpx;
}

.counter-wrap .counter .ico {
  flex: none;
  width: 46rpx;
  height: 46rpx;
  border: 1rpx solid #000;
  border-radius: 50%;
  line-height: 44rpx;
  text-align: center;
  font-size: 44rpx;
}

.btn-confirm {
  width: 100%;
  height: 104rpx;
  line-height: 104rpx;
  margin-top: 60rpx;
  border-radius: 10rpx;
  background: #000;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
}
.icon-crown {
  display: inline-block;

  vertical-align: bottom;
  width: 36rpx;
  height: 30rpx;
  background: url('data:image/png;base64,') no-repeat 0 0/contain;
}