.item {
    height: 150rpx;
    /* line-height: 70rpx; */
    margin: 0rpx 20rpx;
    border-bottom: 1px solid #eee;
    position: relative;
    color: #383838;
}
text {
    margin-right: 30rpx;
    font-size: 30rpx;
}

.goPay {
    display: inline-block;
    width: 120rpx;
    font-size: 30rpx;
    position: absolute;
    right: 30rpx;
    bottom: 10rpx;
}

.no_address {
    position: fixed;
    top: 50%;
    left: 50%;
    margin-top: -100rpx;
    margin-left: -100rpx;
}

.no_address image {
    width: 200rpx;
    height: 200rpx;
}

.no_address view{
    font-size: 28rpx;
    color: #999;
}

.express_title {
    position: relative;
    color: #999;
    font-size: 28rpx;
    margin: 30rpx;
}

.addNewAddress {
    position: fixed;
    bottom: 0;
    height: 140rpx;
    line-height: 140rpx;
    text-align: center;
    font-size: 32rpx;
    width: 100%;
    z-index: 10000;
    border-top: 1rpx solid #ccc;
    background: #fff;

}

.suggestListWrap {
    position: absolute;
    z-index: 5;
    top: 0rpx;
    left: 0;
    right: 0;
    bottom: 100rpx;
    padding-bottom: 30rpx;
    overflow: scroll;
}