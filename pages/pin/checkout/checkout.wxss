page{
    height: 100%;
    background: #fff;
}

.mention-box {
    background: #f4f4f4;
    color: #999999;
    font-size: 24rpx;
    padding: 0rpx 30rpx 30rpx 30rpx;
}

.container {
    height:calc(100vh - 110rpx);
    overflow-y:scroll;

}
.transport-way {
    height: 108rpx;
    position: relative;
    background: #fff;
    padding-top: 21px;
}

.protocol {
    position: fixed;
    bottom: 100rpx;
    background: #fff;
    height: 80rpx;
    line-height: 80rpx;
    padding: 30rpx;
    font-size: 24rpx;
}

.protocol image {
    width: 25rpx;
    height: 25rpx;
    position: relative;
    top: 3rpx;
    left: 5rpx;
}

.protocol .name {
    margin-left: 10rpx;
}

.checkbox {
    float: left;
    height: 34rpx;
    width: 34rpx;
    margin: 25rpx 18rpx 65rpx 0rpx;
    background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/checkbox-0e09baa37e.png) no-repeat;
    background-size: 34rpx;
}

.checkbox.checked {
    background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/checkbox-checked-822e54472a.png) no-repeat;
    background-size: 34rpx;
}


checkbox {
    background: #333;
}

.order-box{
    margin-top: 40rpx;
    width: 100%;
    height: auto;
    overflow: hidden;
    background: #fff;
}

.transport-item {
    position: relative;
    color: #666666;
    font-size: 28rpx;
}

.transport-item .l{
    margin: 40rpx 30rpx;
}

.transport-item .r {
    position: absolute;
    right: 30rpx;
    top: 0rpx;
    /* margin: 30rpx; */
}

.special-item {
    position: relative;
    border-top: 1rpx solid #eee;
    /* color: #666666; */
    font-size: 28rpx;
}

.special-item .l {
    margin: 60rpx 30rpx;
}

.special-item .r {
    position: absolute;
    right: 30rpx;
    top: 50rpx;
    /* margin: 30rpx; */
}

.order-box .order-item{
    /* height: 104.3rpx; */
    overflow: hidden;
    background: #fff;
    display: flex;
    margin-left: 31.25rpx;
    padding-right: 31.25rpx;
    padding-top: 26rpx;
    border-bottom: 1px solid #d9d9d9;
}

.order-box .order-title {
    font-size: 24rpx;
    margin: 40rpx 30rpx 0rpx 30rpx;
}
.order-box .order-item .l{
    display: flex;
    flex-direction: column;
    /* float: left; */
    /* height: 52rpx; */
    width: 35%;
    line-height: 52rpx;
    overflow: hidden;
    font-size: 32rpx;
}

.order-box .order-item .l .format {
    font-size: 20rpx;
    color: #666666;
}

.order-box .order-item .center {
    width: 100rpx;
}
.order-box .order-item .r{
    /* float: right; */
    text-align: right;
    width: 50%;
    height: 52rpx;
    line-height: 52rpx;
    overflow: hidden;
}

.order-box .order-item.no-border{
    border-bottom: none;
}

.goods-items{
    margin-top: 20rpx;
    width: 100%;
    height: auto;
    overflow: hidden;
    background: #fff;
    padding-left: 31.25rpx;
    margin-bottom: 120rpx;
}

.goods-items .item{
    height: 192rpx;
    padding-right: 31.25rpx;
    display: flex;
    align-items: center;
    border-bottom: 1px solid rgba(0,0,0,0.15);
}

.goods-items .item.no-border{
    border-bottom: none;
}


.goods-items .item:last-child{
    border-bottom: none;
}

.goods-items .img{
    height: 145.83rpx;
    width: 145.83rpx;
    background-color: #f4f4f4;
    margin-right: 20rpx;
}

.goods-items .img image{
    height: 145.83rpx;
    width: 145.83rpx;
}

.goods-items .info{
    flex: 1;
    height: 145.83rpx;
    padding-top: 5rpx;
}

.goods-items .t{
    height:  33rpx;
    line-height: 33rpx;
    margin-bottom: 10rpx;
    overflow: hidden;
    font-size: 30rpx;
    color: #333;
}

.goods-items .t .name{
    display: block;
    float: left;
}

.goods-items .t .number{
    display: block;
    float: right;
    margin-right:40rpx;
    text-align: right;
}

.goods-items .m {
    height:  29rpx;
    overflow: hidden;
    line-height: 29rpx;
    margin-bottom: 25rpx;
    font-size: 25rpx;
    color: #666;
}

.goods-items .b {
    height:  41rpx;
    overflow: hidden;
    line-height: 41rpx;
    font-size: 30rpx;
    color: #333;
}

.order-total{
    position: fixed;
    left:0;
    bottom: 0;
    height: 100rpx;
    width: 100%;
    display: flex;
}

.order-total .l{
    flex: 1;
    height: 100rpx;
    line-height: 100rpx;
    color: #7a7a7a;
    background: #fff;
    font-size: 25rpx;
    padding-left: 51.25rpx;
    border-top: 1rpx solid rgba(0,0,0,0.2);
    border-bottom: 1rpx solid rgba(0,0,0,0.2);
}

.order-total .l .price {
    color: #333;
    font-weight: 600;
    font-size: 46rpx;
}

.order-total .r{
    width: 333rpx;
    height: 100rpx;
    background: #333;
    border: 1px solid #b4282d;
    line-height: 100rpx;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
}