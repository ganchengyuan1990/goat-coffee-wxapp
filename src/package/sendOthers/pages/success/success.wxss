.picker {
  display: inline-block;
}

.flex {
  display: flex;
}

.title {
  margin-right: 30rpx;
  display: inline-block;
  min-width: 150rpx;
  text-align: right;
  font-size: 14px;
}

.section {
  margin: 30rpx;
  margin-bottom: 1rem;
  font-size: 14px;
  /* height: 1.8rem; */
  line-height: 1rem;
  margin-left: 0rpx;
  /* margin-bottom: 0.9rem; */
  /* border-bottom: 1rpx solid #eee; */
}

.remark {
  border: 1rpx solid #eee;
  border-radius: 5px;
  margin-left: 5%;
  width: 93%;
  background: rgba(245, 245, 245, 1);
  color: #999999;
  height: 308rpx;
}

.section input {
  width: 70%;
}

.picker {
  position: relative;
  top: -2.34rpx;
}

.changeGender {
  position: relative;
  top: -3rpx;
  display: inline-block;
  width: 50%;
}

.selected {
  /* border-bottom: 1rpx solid #f63; */
}

.button {
  position: fixed;
  bottom: 0rpx;
  /* width: 100%; */
  left: 0rpx;
  right: 0rpx;
  color: #fff;
  display: flex;
  box-shadow: 0px 0px 5px 0px rgba(181, 181, 181, 0.5);

}

.button button {
  border-radius: 0rpx;
  background: #F50000;
  margin: 20rpx 30rpx;
  flex: 1;
  color: #fff;
  margin-right: 15rpx;
  margin-left: 15rpx;
}

.protocol {
  background: #fff;
  height: 80rpx;
  line-height: 80rpx;
  padding: 30rpx;
  font-size: 12px;
  margin-left: 110rpx;
}

.protocol image {
  width: 25rpx;
  height: 25rpx;
  position: relative;
  top: 3rpx;
  left: 5rpx;
}

.protocol .name {
  margin-left: 10rpx;
}

page {
  font-size: 14px;
}

textarea {
  padding: 20rpx;
  font-size: 27rpx;
  line-height: 27rpx;
  width: 95%;
  height: 250rpx;
  /* background:rgba(245, 245, 245, 1); */
}


.line {
  margin: 34rpx;
  padding-bottom: 34rpx;
  border-bottom: 2rpx solid #F5F5F5;
}

.line_title {
  font-size: 28rpx;
  color: #222;
}

.button_line {
  padding-top: 38rpx;
  display: flex;
}

.little_button {
  width: 152rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  text-align: center;
  color: #999999;
  background: #FAFAFA;
  border: 1rpx solid #FAFAFA;
  margin-right: 24rpx;
}

.chosen {
  color: #DF0202;
  background: #fff;
  border: 1rpx solid #FFDBDB;
}

.icons {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 420rpx;
  font-size: 36rpx;
}

.button_line {
  display: flex;
  margin: 30rpx 200rpx;
}

.button_line image {
  display: inline-block;
  /* width: 80%; */
  height: 340rpx;
  border-radius: 20rpx;
  /* margin: 0 10%; */
}

.check-items {
  margin: 0 30rpx;
  line-height: 80rpx;
  padding: 0 10rpx;
  height: 80rpx;
  display: flex;
  border-bottom: 1rpx solid #f5f5f5;
}

.text {
  flex: 1;
}

.input-line {
  overflow: scroll;
  height: calc(100vh - 126rpx);
}

.right {
  margin-right: 0;
}

.success-container {
  position: relative;
  background: rgba(255, 255, 255, 1);
  border-radius: 2px;
  margin: 0 30rpx;
}

.success-form {
  /* width: 100%; */
  background: #fff;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 20rpx;
  display: flex;
  text-align: center;
  flex-direction: column;
  margin-bottom: 40rpx;
}

.cover-image {
  position: fixed;
  bottom: 0;
  height: 300rpx;
  width: 100%;
  z-index: 10;
}

.cover-image .image1{
  display: block;
  width: 100%;
  height: 250rpx;
  border-top-left-radius: 50%;
  border-top-right-radius: 50%;
}

.cover-image .image2 {
  width: 100%;
  height: 50rpx;
  display: block;
}