.header-tabs-container {
  height: 90rpx;
  display: flex;
  justify-content: center;
  font-size: 28rpx;
  background: #FAFAFA;
  border-bottom: 1rpx solid #D8D8D8
}

.header-tabs-container .header-tab-item {
  color: #999;
  font-weight: 700;
  width: 200rpx;
  text-align: center;
  /* margin: 0 100rpx; */
  line-height: 86rpx;

}

.header-tabs-container .tab-item-active {
  color: #F50000;
  border-bottom: 6rpx solid #F50000;
}

.search-find-container {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  /* position: relative;
    top: 0; */
  border-top: 2rpx solid #ccc;
}


.suggestListWrap {
  position: absolute;
  z-index: 5;
  top: 280rpx;
  left: 0;
  right: 0;
  bottom: 100rpx;
  padding-bottom: 30rpx;
  overflow: scroll;
}

.suggestList {
  background-color: #ffffff;
  /* margin-left: 32rpx; */
  overflow: scroll;
  /* box-sizing: border-box; */
  /* height: calc(100vh); */
}

.added {
  position: absolute;
  top: 25rpx;
  right: 30rpx;
  width: 72rpx;
  height: 44rpx;
  line-height: 44rpx;
  border-radius: 22rpx;
  background: #FFDBDB;
  text-align: center;
  font-size: 24rpx;
  color: #F50000;
  border: 1rpx solid #FFDBDB;
}

.higher {
  top: 200rpx;
}

.second {
  margin-left: 10px;
}

.direct-item {
  display: flex;
  padding-bottom: 30rpx;
  padding-top: 30rpx;
}

.direct-item .direct-content {
  display: flex;
  flex-direction: column;
  flex: 1;
  position: relative;
}

.direct-item .direct-name {
  display: flex;
  align-items: center;
}

.direct-item .direct-name-container {
  display: flex;
  flex: 1;
  white-space: nowrap;
  width: 560rpx;
}

.direct-item .direct-name-text {
  font-family: PingFangSC-Regular;
  font-size: 33rpx;
  color: #333333;
  max-width: 68%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.direct-item .text-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.direct-item .important-text {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #FF6633;
}

.direct-item .direct-middle {
  display: flex;
  align-items: center;
  height: 24rpx;
  padding: 4rpx 0;
}

.direct-item .direct-reviewcount-text {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #c6c6c6;
}

.direct-item .direct-bottom {
  display: flex;
  /* align-items: center; */
  flex-direction: column;
}

.direct-item .direct-category {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #c6c6c6;
  flex-shrink: 0;
}

.direct-item .split-line {
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #c6c6c6;
  line-height: 33rpx;
  width: 20rpx;
  text-align: center;
}

.direct-item .direct-address {
  display: flex;
}

.direct-item .direct-address-region {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #c6c6c6;
}

.direct-item .direct-address-text {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  flex: 1;
  max-width: 520rpx;
}

.searchText {
  color: #999;
  margin-left: 20rpx;
  flex: 1;

}

.direct-item .direct-distance {
  display: flex;
}

.direct-item .direct-distance-text {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #c6c6c6;
}


.searchEntry {
  display: flex;
  flex: 1;
  border: 1rpx solid #979797;
  border-radius: 20rpx;
  background-color: #fff;
  position: relative;
  height: 64rpx;
  margin: 10rpx 20rpx;
  color: #111111;
  font-size: 24rpx;
  text-align: left;
  align-items: center;
  justify-content: center;
  margin-top: 50rpx;
  margin-bottom: 50rpx;

}

.searchIcon {
  /* position: absolute;
    left: 0; */
  width: 150rpx;
  text-align: center;
  border-right: 1rpx solid #d8d8d8;
}

.addNewAddress {
  box-shadow: 0px 0px 5px 0px rgba(181, 181, 181, 0.5);
  position: fixed;
  bottom: 0;
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  font-size: 30rpx;
  width: 100%;
  z-index: 10000;
  border-top: 1rpx solid #ccc;
  background: #fff;
  color: #F50000;
}

.suggest-container {
  border-bottom: 2rpx solid #eee;
  margin: 0rpx 30rpx 0rpx 30rpx;

}

.express_title {
  position: relative;
  color: #999;
  font-size: 28rpx;
  margin: 30rpx;
}

.right {
  position: absolute;
  right: 30rpx;
  color: #333;
  top: 0;
}

.no_address {
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -100rpx;
  margin-left: -100rpx;
}

.no_address image {
  width: 200rpx;
  height: 200rpx;
}

.no_address view {
  font-size: 28rpx;
  color: #999;
}

.chosenCity {
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 24rpx;
  color: #999999;
}
.img-wrapper {
  display: flex;
}
.single_item .img-wrapper image {
  width: 300rpx;
  height: 200rpx;
  border-radius: 15rpx;
  /* padding: 30rpx; */
}

.single_item {
  display: inline-block;
  width: 98%;
  margin-left: 1%;
  margin-right: 1%;
  border-radius: 15rpx;
  font-size: 28rpx;
  border: 5rpx solid #eee;
  box-sizing: border-box;
  margin-top: 20rpx;
}

.single_item .info-wrapper {
  margin: 20rpx;
  position: relative;
}

.line {
  text-align: left;
}

.img-intro {
  padding-top: 20rpx;
  padding-left: 30rpx;
}

.righter {
  position: absolute;
  right: 30rpx;
  bottom: 0rpx;
}