page {
  background: #fff !important;
}

.wrapper {
  background: #fafafa;
  padding-bottom: 30rpx;
  height: 100vh;
  overflow: scroll;
}
.promotion-tip {
  position: fixed;
  top: 0;
  width: calc(100% - 120rpx);
  height: 96rpx;
  line-height: 96rpx;
  background-color:#fff;
  color: rgba(102, 102, 102, 1);
  font-size: 26rpx;
  text-align: center;
  display: flex;
  z-index: 100000;
  padding: 0 60rpx;
  /* background: rgb(240, 242, 245); */
  /* text-align: center; */
}
.group-wrapper {
  /* padding: 0 30rpx 0 30rpx; */
}

.promotion-tip text {
  height: 30rpx;
  line-height: 30rpx;
  margin-left: 10rpx;
}

.radio {
  width: 30rpx;
  height: 30rpx;
  line-height: 72rpx;
  display: inline-block;
  border-radius: 50%;
  border: 1rpx solid #ccc;
  position: relative;
  top: 8rpx;
}
.title-box {
  position: relative;
  height: 74rpx;
}
.line {
  width: 100%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  border-bottom: 1rpx solid #e5e5e5;
}
.title {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  padding-left: 35rpx;
  padding-right: 20rpx;
  background: #f6f6f6;
  display: block;
  height: 74rpx;
  line-height: 74rpx;
  font-size: 30rpx;
}
.list-item {
  display: flex;
  flex-direction: column;
  padding: 10rpx;
  justify-content: space-between;
  border-radius: 10rpx;
  background: #fff;
  margin: 40rpx;
  position: relative;
  box-shadow:0rpx 2rpx 10rpx 0rpx rgba(35,24,21,0.2);
  /* padding-bottom: 0; */
  /* padding: 36rpx 15rpx 36rpx 30rpx; */
  /* align-items: center; */
}

.nonono {
  margin-bottom: 0;
}

.row {
  flex-direction: row;
}
.list-item-name {
  display: flex;
  flex-direction: column;
  flex-grow: 5;
  margin-left: 1rem;
}
.promotion-description {
  line-height: 30rpx;
  width: 256rpx;
  font-size: 32rpx;
  font-weight: normol;
  color: #333333;
  margin-bottom: 10rpx;
  max-width: 320rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1; */
  overflow: hidden;
}

.higher {
  margin-bottom: 0rpx;
}
.first-part {
  margin: 0rpx 20rpx 5rpx 20rpx;
  display: flex;
  border-bottom :2rpx solid rgba(229, 229, 229, 1);
  /* flex-direction: column; */
}

.second-part {
