checkbox {
  display: flex;
  margin-top: 2rpx;
  margin-left: 20rpx;
}
checkbox .wx-checkbox-input{
  border-radius: 50%;
  width: 42rpx;
  height: 42rpx;
  background: #fff;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
  border: none;
  background: #DE4132;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
  border-radius: 50%;
  width: 42rpx;
  height: 42rpx;
  line-height: 46rpx;
  text-align: center;
  font-size: 25rpx;
  color: #ffffff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
}

