.hfe-vue-dialog-backdrop {
  position: fixed;
  z-index: 9998;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  transition: opacity 0.3s ease;
}
.hfe-vue-dialog-toast-content-backdrop .wc {
  display: inline-block;
  padding: 20rpx 80rpx;
  background-color: #585858;
  border-radius: 15rpx;
  color: #fff;
  font-size: 28rpx;
}
.hfe-vue-dialog-toast-wrapper {
  display: flex;
  max-width: 95%;
  margin: auto;
  height: 100%;
  align-items: center;
  justify-content: center;
}
.sure-button {
  /* position: fixed;
  bottom: 0; */
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  z-index: 9999;
  background: red;
}

.protocol {
  /* position: fixed;
  bottom: 100rpx; */
  z-index: 9999;
  font-size: 32rpx;
  height: 150rpx;
  line-height: 150rpx;
  background: white;
  width: 100%;
}

.protocol image {
  width: 32rpx;
  height: 32rpx;
  position: relative;
  top: 8rpx;
  left: 5rpx;
  margin-right: 10rpx;
}

.bottom-position {
  position: fixed;
  bottom: 0;
  width: 100%;
  display: flex;
  flex-direction: column;
  background: #fff;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
}