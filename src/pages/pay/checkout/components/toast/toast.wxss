.hfe-vue-dialog-backdrop {
  position: fixed;
  z-index: 9998;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  transition: opacity 0.3s ease;
}
.hfe-vue-dialog-toast-content-backdrop .wc {
  display: inline-block;
  padding: 20rpx 80rpx;
  background-color: #585858;
  border-radius: 15rpx;
  color: #fff;
  font-size: 28rpx;
}
.hfe-vue-dialog-toast-wrapper {
  display: flex;
  max-width: 95%;
  margin: auto;
  height: calc(100vh - 320rpx);
  align-items: center;
  justify-content: center;
}
.sure-button {
  /* position: fixed;
  bottom: 0; */
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
  z-index: 9999;
  background: #DE4132;
}

.protocol {
  /* position: fixed;
  bottom: 100rpx; */
  z-index: 9999;
  font-size: 32rpx;
  height: 130rpx;
  line-height: 110rpx;
  margin-left: 30rpx;
  background: white;
  width: 100%;
}

.protocol image {
  width: 32rpx;
  height: 32rpx;
  position: relative;
  top: 5rpx;
  left: 3rpx;
  margin-right: 8rpx;
}

.bottom-position {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 310rpx;
  display: flex;
  flex-direction: column;
  background: #fff;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
}

.bottom-title {
  margin: 30rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #232323;
  margin-bottom: 10rpx;
}

.bottom-sub-title {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #B2B2B2;
  margin-left: 10rpx;
}