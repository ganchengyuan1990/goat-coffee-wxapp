    var checkout = {
        isGotType: function (pickUps, type) {
            console.log(pickUps)
            var flag = false;
            var type = pickUps.map(function(item) {
                if (item.type === type) {
                    flag = true;
                }
                return item;
            });
            return flag;
        },
        getPriceSimple: function (value) {
            var stringValue = value.toString();
            if(stringValue.indexOf('.') < 0) {
                return value;
            } else {
                return parseFloat(stringValue).toFixed(2);
            }
        }
    }
    module.exports = checkout