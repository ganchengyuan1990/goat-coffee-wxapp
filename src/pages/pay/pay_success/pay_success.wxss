.award-image {
  position: absolute;
  margin: auto;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -50%);
  height: 70rpx;
  width: 110rpx;
}

.award-title {
  position: absolute;
  margin: auto;
  bottom: 5%;
  left: 50%;
  transform: translate(-50%, 0);
  font-size:20rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(150,124,31,1);
  width: 160rpx;
  text-align: center;
}

.animation_title {
  animation: 3s rowup linear infinite normal;
  overflow: hidden;
}

@keyframes rowup {
  0%   {opacity: 1;}
  80%  {opacity: 1;}
  100% {opacity: 0;}
}

