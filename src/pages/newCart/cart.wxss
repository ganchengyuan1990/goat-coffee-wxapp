.cart-view .item .cart-goods{
  position: relative;
    height:16rpx;
    display: flex;
    /* border-bottom: 1px solid #f4f4f4; */
}

.cart-view .item .img{
    float: left;
    height:125rpx;
    width: 125rpx;
    background: #f4f4f4;
    margin: 19.5rpx 18rpx 19.5rpx 0;
}

.cart-view .item .info{
    float: left;
    height:146rpx;
    display: flex;
    flex-direction: column;
    /* align-items: center; */
    justify-content: center;
    margin-right: 18rpx;
    margin-left: 30rpx;
    /* flex: 1; */
    /* margin: 15rpx 16rpx 1rpx 0rpx; */
}

.cart-view .item .t{
    margin: 12rpx 0 20rpx 0;
    height: 38rpx;
    line-height: 38rpx;
    font-size: 30rpx;
    color: #333;
    overflow: hidden;
    position: relative;
    display: flex;
    align-items: center;
    margin-bottom: 0rpx;
}


.cart-view .item .name{
    height: 35rpx;
    max-width: 310rpx;
    line-height: 30rpx;
    font-size: 30rpx;
    color: #232323;
    font-weight:500;
    overflow: hidden;
}

.cart-view .item .num{
    height: 28rpx;
    line-height: 28rpx;
    float: right;
}

.not_deliver {
    background: rgba(245, 0, 0, 0.3);
    opacity: 0.4;
}

.not_online {
    /* background: #eee; */
    opacity: 0.4;
}
.grrr {
    color: #B3B3B3 !important;
}
.cart-view .item .attr{
    margin-top: 5rpx;
    margin-bottom: 5rpx;
    height: 24rpx;
    line-height: 24rpx;
    font-size: 18rpx;
    color: #DAA37F;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
    max-width:218rpx;
}

.cart-view .item .b{

  line-height: 32rpx;
    margin-right: 20rpx;
    /* height: 28rpx;
    line-height: 28rpx; */
    font-size: 32rpx;
    color: #333;
    overflow: hidden;
    font-weight: 500;
    display: flex;
    margin-top: 16rpx;
}

.cart-view .item .price{
    float: left;
    font-size: 32rpx;
}

.cart-view .item .open{
    height: 28rpx;
    width: 150rpx;
    display: block;
    float: right;
    background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/arrowDown-d48093db25.png) right center no-repeat;
    background-size: 25rpx;
    font-size: 25rpx;
    color: #333;
}

.cart-view .item.edit .t{
    display: none;
}

.cart-view .item.edit .attr{
    text-align: right;
    background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/icon-normal/arrow-right1-e9828c5b35.png) right center no-repeat;
    padding-right: 25rpx;
    background-size: 12rpx 20rpx;
    margin-bottom: 24rpx;
    height: 39rpx;
    line-height: 39rpx;
    font-size: 24rpx;
    color: #999;
    overflow: hidden;
}

.cart-view .item.edit .b{
    display: flex;
    height: 62rpx;
    overflow: hidden;
}

.cart-view .item.edit .price{
    line-height: 52rpx;
    height: 52rpx;
    flex: 1;
}

.cart-view .item .selnum{
    display: none;
}

.cart-view .item.edit .selnum{
    width: 235rpx;
    height: 52rpx;
    border: 1rpx solid #ccc;
    display: flex;
}

.selnum .cut{
    width: 70rpx;
    height: 100%;
    text-align: center;
    line-height: 50rpx;
}

.selnum .number{
    flex: 1;
    height: 100%;
    text-align: center;
    line-height: 68.75rpx;
    border-left: 1px solid #ccc;
    border-right: 1px solid #ccc;
    float: left;
}

.selnum .add{
    width: 80rpx;
    height: 100%;
    text-align: center;
    line-height: 50rpx;
}


.cart-view .group-item .header{
    width: 100%;
    height: 94rpx;
    line-height: 94rpx;
    padding: 0 26rpx;
    border-bottom: 1px solid #f4f4f4;
}

.cart-view .promotion .icon{
    display: inline-block;
    height: 24rpx;
    width: 15rpx;
}

.cart-view .promotion{
    margin-top: 25.5rpx;
    float: left;
    height: 43rpx;
    width: 480rpx;
    /*margin-right: 84rpx;*/
    line-height: 43rpx;
    font-size: 0;
}

.cart-view .promotion .tag{
    border: 1px solid #f48f18;
    height: 37rpx;
    line-height: 31rpx;
