.direct-item {
    display: flex;
    padding-bottom: 30rpx;
    padding-top: 30rpx;
}

.direct-item .direct-content {
    display: flex;
    flex-direction: column;
    flex: 1;
    position: relative;
}

.direct-item .direct-name {
    display: flex;
    align-items: center;
}

.direct-item .direct-name-container {
    display: flex;
    flex: 1;
    white-space: nowrap;
    width: 560rpx;
}

.direct-item .direct-name-text {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #333333;
    max-width: 68%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.direct-item .text-overflow {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.direct-item .important-text {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #FF6633;
}

.direct-item .direct-middle {
    display: flex;
    align-items: center;
    height: 24rpx;
    padding: 4rpx 0;
}

.direct-item .direct-reviewcount-text {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #c6c6c6;
}



.direct-item .direct-category {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #999;
    flex-shrink: 0;
}
.lalala {
  margin: 10rpx 0;
}
.direct-item .split-line {
    font-family: PingFangSC-Regular;
    font-size: 20rpx;
