    text-align: center;
}

.direct-item .direct-address {
    display: flex;
    /* margin-left: 20rpx; */
}

.direct-item .direct-address-region {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #c6c6c6;
}

.direct-item .direct-address-text {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #999;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    flex: 1;
    max-width: 600rpx;
}
