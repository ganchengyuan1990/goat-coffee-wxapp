.act-wrap {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 998;
}

.act-wrap .mask {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0, 0, 0, 0.8);
    pointer-events: none;
    opacity: 0;
    transition: all .5s;
}

.act-wrap .mask.active {
    opacity: 0.9;
}

.act-wrap .cnt {
    position: absolute;
    top: 0;
    left: 15%;
    width: 70%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.act-wrap .get_button {
    margin-top: 30rpx;
    width:210rpx;
    height:90rpx;
    line-height:90rpx;
    border-radius:8rpx;
    border:1rpx solid rgba(151, 151, 151, 1);
    color: #fff;
    text-align: center;
}

.act-wrap .cnt image {
    width: 100%;
    height: auto;
}

.act-wrap .btn-close {
    align-self: flex-end;
    position: relative;
    width: 64rpx;
    height: 64rpx;
    background: url('data:image/png;base64,') no-repeat center center/contain;
    transform: translateY(-40rpx);
}

