page {
  background-color: #fff;
}
.store-container {
  /* display: flex; *//* flex-direction: column; */
  height: 100vh;
  overflow: hidden;
}
.pad-nav {
  margin-bottom: 100rpx;
}
.hd-wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 124rpx;
  /* padding-right: 30rpx; */
  /* border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx; */
  position: relative;
  /* background: #eee; */
  z-index: 10000;
}

.hd-wrapper {
  z-index: 0;
  position: relative;
}

.delivery {
  position: absolute;
  right: 30rpx;
}

.img-wrap {
  width: 100%;
  height: 240rpx;
  position: relative;
  /* z-index: 100; */
}
.img-wrap swiper {
  height: 240rpx;
}
.img-caption {
  display: block;
  width: 100%;
  height: 240rpx;
  position: relative;
  z-index: 100;
}
.info-wrap {
  height: 50rpx;
  padding: 0 10rpx;
  background: #fef9d9;
  font-size: 24rpx;
  font-weight: normal;
  line-height: 50rpx;
  color: #999;
}
.store-addr {
  flex: none;
  max-width: 60%;
  padding-left: 58rpx;
  font-size: 24rpx;
  line-height: 1.4;

}

.store-addr .tt {
  position: relative;
  top: 0rpx;
}
.store-addr .tt .addr {
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow:ellipsis;
  overflow: hidden;
  font-size: 28rpx;
  font-weight:500;
}
.store-addr .tt::before {
  content: "";
  position: absolute;
  left: -12rpx;
  top: 66%;
  width: 20rpx;
  height: 24rpx;
  background: url('data:image/png;base64,') no-repeat 0 0/contain;
  transform: translate(-100%, -50%);
}

.tt_high:before {
  top: 30%;
  margin-top: -10rpx;
}


.distance {
  color: rgba(0,0,0,0.2);
  font-size: 24rpx;
  margin-top: 5rpx;
}

.btn-delivery {
  flex: none;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 0rpx;
  /* border: 2rpx solid rgba(35,24,21,1); */
  border-radius: 10rpx;
  color: #232323;
  font-size: 26rpx;
}

.btn-delivery .item {
  padding: 18rpx 20rpx;
  border-radius:4rpx 0rpx 0rpx 4rpx;
  line-height: 1;
  font-weight:500;
}

.btn-delivery .item.active {
  color: #D32D25;
  background: #231815;
}

.list-wrap {
  display: flex;
  height: 100%;
  /* box-shadow: 0px 2rpx 6rpx 0px rgba(35, 24, 21, 0.3); */
}

.list-nav {
  flex: none;
  width: 184rpx;
  background-color: #f6f6f6;
}

.list-nav .active {
  background-color: #fff;
}

.list-nav .list-nav-item {
  position: relative;
  height: 100rpx;
  box-sizing: border-box;
  padding: 10rpx 8rpx;
  font-size: 26rpx;
  line-height: 40rpx;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  color: #999999;
  justify-content: center;
}

.list-nav .list-nav-item:last-child {
  margin-bottom: 120rpx;
}
.list-nav .list-nav-item .icon-crown {
  display: none;
}
.list-nav .list-nav-item.active .icon-crown {
  display: inline-block;
}
.list-nav .list-nav-item .tt {
  text-align: center;
  max-height: 80rpx;
  overflow: hidden;
  line-height: 1.2;
  color: #787878;
}
.list-nav .list-nav-item.active .tt {
  color: #222222;
}
.list-cnt {
  flex-grow: 1;
  padding-left: 20rpx;
  font-size: 20rpx;
}

.list-cnt .title {
  display: flex;
  align-items: center;
  margin: 34rpx 30rpx 30rpx 0;
  color: #d8d8d8;
  font-size: 22rpx;
  color: #353535;
  font-weight:500;
}

.first_title {
  margin-top: 30rpx !important;
}

.lowersss {
  margin-bottom: 20rpx !important;
}
.list-cnt .title .line {
  flex: 1;
  height: 2rpx;
  margin: 0 20rpx;
  background: #f2f2f2;
}
/* .list-cnt .title-group {
  padding-bottom: 30rpx;
} */

.list-cnt .title-group:last-child {
  margin-bottom: 480rpx;
}

.list-cover {
  height: 0rpx;
  position: relative;
  z-index: 1000;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.list-detail {
  margin-bottom: 28rpx;
  display: flex;
  /* opacity: 0.8; */
}

.list-detail:last-child {
  margin-bottom: 20rpx;
}

.list-detail .img {
  margin-right: 24rpx;
  position: relative;
}

.list-detail .img image {
  display: block;
  width: 140rpx;
  height: 140rpx;
  border-radius: 8rpx;
}

.list-detail .info {
  width: 170rpx;
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.info .name {
  height: 30rpx;
  line-height: 30rpx;
  font-size: 30rpx;
  /* font-weight: 600; */
  margin-bottom: 5rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight:500;
}

.info .note {
  height: 28rpx;
  padding-right: 8rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 20rpx;
  line-height: 1.4;
  color: #c6c6c6;
  overflow: hidden;
}

.price-count {
  position: relative;
  /* display: flex; */
  /* align-items: center; */
  /* height: 50rpx;
  line-height: 50rpx; */
}

.price-count .price {
  font-size: 30rpx;
  line-height: 1;
  flex-grow: 1;
  color: #000;
  font-weight: 600;
  margin-left: -5rpx;
}
.price-count .price-del {
  margin-left: 8rpx;
  font-size: 28rpx;
  color: rgba(35,35,35,0.6);
  text-decoration: line-through;
}
.price-count .count {
  display: flex;
  line-height: 60rpx;
  flex-grow: 1;
  flex-direction: row-reverse;
}

.price-count .count .num {
  line-height: 50rpx;
  font-size: 30rpx;
}
.price-count .btn {
  position: absolute;
  right: 40rpx;
  top: 40%;
  transform: translateY(-50%);
}
.btn {
  position: relative;
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  /* font-size: 36rpx; */
  font-weight: 400;
  color: #fff;
  text-align: center;
  line-height: 30rpx;
  /* background-color: rgb(253,133,133); */
}
.btn .plus {
  transform: scale(1.6);
}
.price-wrap .price {
  font-size: 36rpx;
}

.price-wrap .desc {
  font-size: 20rpx;
  color: #c6c6c6;
}

.counter-wrap {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-top: 38rpx;
}

.counter-wrap .counter {
  display: flex;
  align-items: center;
}

.counter-wrap .counter .num {
  padding: 0 20rpx;
  font-size: 22rpx;
}

.counter-wrap .counter .ico {
  flex: none;
  width: 46rpx;
  height: 46rpx;
  border: 1rpx solid #000;
  border-radius: 50%;
  line-height: 44rpx;
  text-align: center;
  font-size: 44rpx;
}

.btn-confirm {
  width: 100%;
  height: 104rpx;
  line-height: 104rpx;
  margin-top: 60rpx;
  border-radius: 10rpx;
  background: #000;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
}
.icon-crown {
  display: inline-block;
  vertical-align: bottom;
  width: 36rpx;
  height: 30rpx;
  background: url('data:image/png;base64,') no-repeat 0 0/contain;
}
.act-wrap {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 998;
}
.act-wrap .mask {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: rgba(0, 0, 0, 0.8);
    pointer-events: none;
    opacity: 0;
    transition: all .5s;
}
.act-wrap .mask.active {
  opacity: 1;
}
.act-wrap .cnt {
  position: absolute;
  top: 0;
  left: 15%;
  width: 70%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.act-wrap .cnt image {
  width: 100%;
  height: auto;
}
.act-wrap .btn-close {
  align-self: flex-end;
  position: relative;
  width: 64rpx;
  height: 64rpx;
  background: url('data:image/png;base64,') no-repeat center center/contain;
  transform: translateY(-40rpx);
}

.discount {
  width: 38rpx !important;
  position: absolute !important;
  top: -1rpx !important;
  left: 10rpx !important;
  display: inline-block;
  overflow: hidden;
