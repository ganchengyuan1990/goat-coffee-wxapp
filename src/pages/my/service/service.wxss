.panel {
    margin: 0rpx 30rpx 40rpx 30rpx;
    padding: 0 30rpx;
    background: #fff;
    color: #999;
    font-size: 22rpx;
    position: relative;
    box-shadow:0px 2rpx 6rpx 0rpx rgba(35,24,21,0.2);
    border-radius:8rpx;
}
.panel .panel-hd {
    display: flex;
    justify-content: space-between;
    padding: 40rpx 0 20rpx 0;
    border-bottom: 2rpx solid #f2f2f2;
    line-height: 1;
}
.panel .panel-hd .active {
    position: relative;
    top: 10rpx;
    color: #7ED321;
}
.panel .panel-hd .tt {
    width: 80%;
    overflow: hidden;
    text-overflow: ellipsis;
    /* margin-top: 30rpx; */
    color: #333;
    font-size: 28rpx;
    line-height: 40rpx;
}

.panel .panel-bd {
    display: flex;
    align-items: center;
    margin-top: 20rpx;
}
.panel .panel-bd .desc {
    margin-top: 8rpx;
    line-height: 32rpx;
}
.panel .panel-bd .date {
    color: #ccc;
    margin-top: 16rpx;
    line-height: 32rpx;
}
.panel .panel-ft {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    padding: 20rpx 0 20rpx;
}
.panel .panel-ft .price {
    color: #000;
    font-size: 36rpx;
    line-height: 50rpx;
}
