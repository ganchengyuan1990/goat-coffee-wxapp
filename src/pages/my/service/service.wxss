@import '../../my/common/list.wxss';

page {
    height: 100%;
    background: #fff;
}
.order-wrap {
    display: flex;
    flex-direction: column;
    height: 100vh;
    background: #eee;
}
.filter-wrap {
    display: flex;
    flex-direction: column;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    padding: 0 100rpx;
    /* height: 80rpx; */
    /* background: #fff; */
    z-index: 10;
    height: 122rpx;
    background: #fff;
}
.filter-wrap.active {
    bottom: 0;
    background: rgba(0, 0, 0, .5);
}
.filter-wrap .tab {
    flex: none;
    display: flex;
    padding: 20rpx 30rpx;
    color: #666;
    background: #fff;
    font-size: 28rpx;
    line-height: 40rpx;
}
.filter-wrap .tab .item {
    position: relative;
}
.filter-wrap .tab .item:first-child {
    margin-right: 120rpx;
}
.filter-wrap .tab .item text {
    content: "";
    display: block;
    position: absolute;
    top: 50%;
    right: -20rpx;
    border-bottom: 8rpx solid #ccc;
    border-left: 6rpx solid transparent;
    border-right: 6rpx solid transparent;
    transform: translateY(-50%);
}
.filter-wrap .tab .item.active text {
    border-bottom-color: #333;
    transform: translateY(-50%) rotate(180deg);
}
.filter-wrap .cnt {
    flex: none;
    height: 200rpx;
    padding: 20rpx 30rpx;
    background: #fff;
    color: #9b9b9b;
    font-size: 28rpx;
    line-height: 40rpx;
    box-sizing: border-box;
}
.filter-wrap .cnt .category .item {
    padding: 20rpx 0;
}
.filter-wrap .cnt .type {
    padding-top: 40rpx;
}
.filter-wrap .cnt .type .item {
    display: inline-block;
    margin-right: 48rpx;
    padding: 8rpx 20rpx;
    border: 2rpx solid #979797;
    border-radius: 4rpx;
    color: #666;
    line-height: 40rpx;
}
.filter-wrap .cnt .active {
    color: #000;
}
.filter-wrap .cnt .type .active {
    background: #f4f4f4;
}
.filter-wrap .mask {
    flex: 1;
}
.fixed {
    /* height: 100%;     */
    /* margin-top: 145rpx; */
    position: fixed;
    top: 132rpx;
    z-index: 10000;
    width: 100%;
    background: #fff;
    margin-top: 0 !important;
