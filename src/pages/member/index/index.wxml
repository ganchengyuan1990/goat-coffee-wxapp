<view class="act-wrap" wx:if="{{isActWrapShow}}" catchtap="closeToast">
    <view class="mask {{isActWrapShow ? 'active':''}}" catchtap="closeToast"></view>
    <view class="cnt">
        <view class="btn-close" catchtap="closeToast"></view>
        <image src="{{actImage}}" mode="widthFix" bindtap="handleImage"></image>
    </view>
</view>

