                <view class="panel-hd">
                    <view class="tt">{{item.order.storeName}}<text wx:if="{{item.order.storeCoffeeMakerId}}">（编号{{item.order.storeCoffeeMakerId}}）</text><text wx:if="{{item.order.orderType === 1}}">(NO.{{item.order.varCode}})</text></view>
