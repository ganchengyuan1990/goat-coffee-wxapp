<view class="center" bind:tap="goProfile" wx:if="{{hasLogin}}">
    <image src="{{img}}"></image>
    <view class='personal'>
        <view class="nick">{{name}}</view>
    </view>
</view>


<view class="center" bind:tap="onClickAvatar" wx:if="{{!hasLogin}}">
