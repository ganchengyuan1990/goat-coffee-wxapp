  /* transform: translate3d(0, 100%, 0); */
  /* animation: rise 3s cubic-bezier(0.19, 1, 0.22, 1) 0.25s forwards; */
}


.remind-box {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding-bottom: 300rpx;
}

.remind-img {
  width: 250rpx;
  height: 200rpx;
  padding-bottom: 25rpx;
}

@keyframes rise {
  0% {
    opacity: 0;
    transform: translateY(300rpx);
  }

  50% {
    opacity: 1;
  }

  100% {
    opacity: 1;
    transform: translateY(350rpx);
  }
}


