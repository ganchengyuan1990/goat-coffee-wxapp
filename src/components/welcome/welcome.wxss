.wave {
  position: absolute;
  z-index: 3;
  right: 0;
  top: 0rpx;
  opacity: 0.725;
  height: 160rpx;
  width: 100%;
  /* height: 570rpx;
  width: 2250rpx; */
  /* animation: wave 3s linear infinite; */
}

.wave100 {
  animation: wave100 2s linear infinite;
}


.wave90 {
  animation: wave90 2s linear infinite;
}

.wave80 {
  animation: wave80 2s linear infinite;
}

.wave70 {
  animation: wave70 2s linear infinite;
}

.wave60 {
  animation: wave60 2s linear infinite;
}

.wave50 {
  animation: wave50 2s linear infinite;
}

.wave40 {
  animation: wave40 2s linear infinite;
}

.wave30 {
  animation: wave30 2s linear infinite;
}

.wave20 {
  animation: wave20 2s linear infinite;
}

.wave10 {
  animation: wave10 2s linear infinite;
}

@keyframes wave100 {
  from {
    transform: translateY(15rpx);
  }

  to {
    transform: translateY(0rpx);
  }
}

@keyframes wave90 {
  from {
    transform: translateY(30rpx);
  }

  to {
    transform: translateY(15rpx);
  }
}

@keyframes wave80 {
  from {
    transform: translateY(45rpx);
  }

  to {
    transform: translateY(30rpx);
  }
}

@keyframes wave70 {
  from {
    transform: translateY(60rpx);
  }

  to {
    transform: translateY(45rpx);
  }
}

@keyframes wave60 {
  from {
    transform: translateY(75rpx);
  }

  to {
    transform: translateY(60rpx);
  }
}

@keyframes wave50 {
  from {
    transform: translateY(90rpx);
  }

  to {
    transform: translateY(75rpx);
  }
}

@keyframes wave40 {
  from {
    transform: translateY(105rpx);
  }

  to {
    transform: translateY(90rpx);
  }
}

@keyframes wave30 {
  from {
    transform: translateY(120rpx);
  }

  to {
    transform: translateY(105rpx);
  }
}

@keyframes wave20 {
  from {
    transform: translateY(135rpx);
  }

  to {
    transform: translateY(120rpx);
  }
}

@keyframes wave10 {
  from {
    transform: translateY(150rpx);
  }

  to {
    transform: translateY(135rpx);
  }
}


