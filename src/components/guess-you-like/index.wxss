page {
  height: 100%;
  min-height: 100%;
  background: #f6f6f6;
}



.cart-view {
  width: 100%;
  height: auto;
  overflow: hidden;

}

.cart-view .list {
  height: auto;
  width: 100%;
  overflow: hidden;
  margin-bottom: 120rpx;
}

.cart-view .group-item {
  height: auto;
  width: 100%;
  /* background: #fff; */
  margin-bottom: 18rpx;
}

.cart-view .item {
  height: 164rpx;
  width: 100%;
  overflow: hidden;
}

.cart-view .item .checkbox {
  float: left;
  height: 34rpx;
  width: 34rpx;
  margin: 65rpx 18rpx 65rpx 26rpx;
  background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/checkbox-0e09baa37e.png) no-repeat;
  background-size: 34rpx;
}

.cart-view .item .checkbox.checked {
  background: url(http://img.jasongan.cn/checked.png) no-repeat;
  background-size: 34rpx;
}

.guige {
  /* margin-left: 30rpx; */
  font-size: 24rpx;
  color: #999;
}

.cart-view .item .cart-goods {
  position: relative;
  height: 164rpx;
  display: flex;
  border-bottom: 1px solid #f4f4f4;
}

.cart-view .item .img {
  float: left;
  height: 125rpx;
  width: 125rpx;
  background: #f4f4f4;
  margin: 19.5rpx 18rpx 19.5rpx 0;
}

.cart-view .item .info {
  float: left;
  height: 125rpx;
  /* flex: 1; */
  margin: 31rpx 16rpx 19.5rpx 0rpx;
}

.cart-view .item .t {
  margin: 8rpx 0;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 30rpx;
  color: #333;
  overflow: hidden;
}


.cart-view .item .name {
  height: 28rpx;
  max-width: 310rpx;
  line-height: 28rpx;
  font-size: 25rpx;
  color: #333;
  overflow: hidden;
}

.cart-view .item .num {
  height: 28rpx;
  line-height: 28rpx;
  float: right;
}

.not_deliver {
  background: rgba(245, 0, 0, 0.3);
  opacity: 0.4;
}

.not_online {
  background: #eee;
  opacity: 0.4;
}

.cart-view .item .attr {
  margin-top: 15rpx;
  margin-bottom: 17rpx;
  height: 24rpx;
  line-height: 24rpx;
  font-size: 22rpx;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 240rpx;
}

.cart-view .item .b {
  po line-height: 30rpx;
  margin-right: 40rpx;
  /* height: 28rpx;
    line-height: 28rpx; */
  font-size: 28rpx;
  color: #333;
  overflow: hidden;
  font-weight: 500;
}

.cart-view .item .price {
  float: left;
}

.cart-view .item .open {
  height: 28rpx;
  width: 150rpx;
  display: block;
  float: right;
  background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/arrowDown-d48093db25.png) right center no-repeat;
  background-size: 25rpx;
  font-size: 25rpx;
  color: #333;
}

.cart-view .item.edit .t {
  display: none;
}

.cart-view .item.edit .attr {
  text-align: right;
  background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/icon-normal/arrow-right1-e9828c5b35.png) right center no-repeat;
  padding-right: 25rpx;
  background-size: 12rpx 20rpx;
  margin-bottom: 24rpx;
  height: 39rpx;
  line-height: 39rpx;
  font-size: 24rpx;
  color: #999;
  overflow: hidden;
}

.cart-view .item.edit .b {
  display: flex;
  height: 62rpx;
  overflow: hidden;
}

.cart-view .item.edit .price {
  line-height: 52rpx;
  height: 52rpx;
  flex: 1;
}

.cart-view .item .selnum {
  display: none;
}

.cart-view .item.edit .selnum {
  width: 235rpx;
  height: 52rpx;
  border: 1rpx solid #ccc;
  display: flex;
}

.selnum .cut {
  width: 70rpx;
  height: 100%;
  text-align: center;
  line-height: 50rpx;
}

.selnum .number {
  flex: 1;
  height: 100%;
  text-align: center;
  line-height: 68.75rpx;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  float: left;
}

.selnum .add {
  width: 80rpx;
  height: 100%;
  text-align: center;
  line-height: 50rpx;
}


.cart-view .group-item .header {
  width: 100%;
  height: 94rpx;
  line-height: 94rpx;
  padding: 0 26rpx;
  border-bottom: 1px solid #f4f4f4;
}

.cart-view .promotion .icon {
  display: inline-block;
  height: 24rpx;
  width: 15rpx;
}

.cart-view .promotion {
  margin-top: 25.5rpx;
  float: left;
  height: 43rpx;
  width: 480rpx;
  /*margin-right: 84rpx;*/
  line-height: 43rpx;
  font-size: 0;
}

.cart-view .promotion .tag {
  border: 1px solid #f48f18;
  height: 37rpx;
  line-height: 31rpx;
  padding: 0 9rpx;
  margin-right: 10rpx;
  color: #f48f18;
  font-size: 24.5rpx;
}

.cart-view .promotion .txt {
  height: 43rpx;
  line-height: 43rpx;
  padding-right: 10rpx;
  color: #333;
  font-size: 29rpx;
  overflow: hidden;
}

.cart-view .get {
  margin-top: 18rpx;
  float: right;
  height: 58rpx;
  padding-left: 14rpx;
  border-left: 1px solid #d9d9d9;
  line-height: 58rpx;
  font-size: 29rpx;
  color: #333;
}

.cart-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 96rpx;
  width: 100%;
  background: #fff;
  display: flex;
  z-index: 1000;
}



.title {
  position: absolute;
  left: 30rpx;
  /* transform: translateX(-50%); */
  /* padding-left: 35rpx; */
  /* background: #f6f6f6; */
  display: block;
  height: 94rpx;
  line-height: 94rpx;
  color: #231815;
  font-size: 30rpx;
}

.number {
  position: absolute;
  right: 30rpx;
  top: 25rpx;
  color: #A5A5A5;
  font-size: 20rpx;
}

.title-box {
  position: relative;
  height: 74rpx;
}

.scroll_view {
  display: flex;
  flex-direction: row;
  overflow: hidden;
  white-space: nowrap;
}

.add_button {
  color: rgb(253, 133, 133);
  border: 1rpx solid rgb(253, 133, 133);
  border-radius: 8rpx;
  text-align: center;
  width: 200rpx;
  font-size: 28rpx;
  line-height: 80rpx;
  height: 80rpx;
}

.img_wrapper {
  height: 150rpx;
  width: 90%;
  margin-left: 5%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: linear-gradient(130deg, rgba(95, 65, 56, 1) 0%, rgba(185, 131, 96, 1) 100%);
  border-radius: 14px;
  margin-top: 20rpx;
}

.img_wrapper image {
  height: 100%;
  width: 100%;
  border-radius: 14px;
}

.good_img {
  width: 120rpx;
  height: 120rpx;
  background: rgba(216, 216, 216, 1);
  box-shadow: 0px 2rpx 10rpx 0px rgba(35, 24, 21, 0.3);
  border-radius: 70rpx;
}

.good_img image {
  height: 100%;
  width: 100%;
  border-radius: 70rpx;
}

.first {
  margin: 31rpx 6rpx 19.5rpx 0rpx;
}

.scroll-list {
  display: scroll;
}

.img_view {
  width: 259rpx;
  height: 184rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 2rpx 10rpx 0rpx rgba(35, 24, 21, 0.14);
  border-radius: 10rpx 10rpx 0rpx 0rpx;
}

.caini_item {
  width: 259rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 2rpx 10rpx 0rpx rgba(35, 24, 21, 0.14);
  border-radius: 10rpx;
  margin: 30rpx;
  min-height: 316rpx;
  position: relative;
  flex: 1;
  display: inline-block;
}

.img_view image {
  height: 100%;
  width: 100%;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
}

.content_view {
  font-size: 28rpx;
}


.btn {
  position: relative;
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  /* border: 3.3rpx solid #f63; */
  margin-top: 6.2rpx;
}

.btn:before {
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  width: 20rpx;
  height: 3.3rpx;
  background-color: rgb(253, 133, 133);
  border-radius: 1.7rpx;
  transform: translate(-50%, -50%);
}

.btn-plus:after {
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  width: 3.3rpx;
  height: 22rpx;
  background-color: rgb(253, 133, 133);
  border-radius: 1.7rpx;
  transform: translate(-50%, -50%);
}

.btn-disable {
  border-color: #ececec;
}

.btn-disable:before,
.btn-disable:after {
  background-color: #ececec;
}

.content {
  position: absolute;
  bottom: 10rpx;
  width: 100%;
}

.float_right {
  position: absolute;
  right: 20rpx;
  bottom: 0rpx;
}

.del {
  color: rgba(191, 191, 191, 1);
  text-decoration: line-through;
  font-size: 20rpx;
  margin-left: 5rpx;
}